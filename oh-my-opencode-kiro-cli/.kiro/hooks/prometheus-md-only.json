  "name": "prometheus-md-only",
  "description": "Enforce markdown-only output for Prometheus planner",
  "event": "PostToolUse",
  "trigger": {
    "type": "manual",
    "command": "prometheus-md-only"
  },
  "agent": "sisyphus",
  "enabled": true,
  "metadata": {
    "omoc_event": "PostToolUse"
  }
}