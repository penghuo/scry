{
  "name": "sisyphus",
  "model": "anthropic/claude-opus-4-5",
  "temperature": 0.1,
  "prompt": "file://../prompts/sisyphus.md",
  "tools": [
    "@builtin",
    "subagent"
  ],
  "toolsSettings": {
    "write": {
      "allowedPaths": [
        "**/*"
      ]
    },
    "shell": {
      "allowedCommands": [
        "npm *",
        "yarn *",
        "pnpm *",
        "cargo *",
        "python *",
        "pip *",
        "go *",
        "make *",
        "rg *",
        "grep *",
        "find *",
        "ast-grep *",
        "cat *",
        "ls *",
        "tree *",
        "head *",
        "tail *",
        "wc *",
        "mkdir *",
        "touch *",
        "cp *",
        "mv *"
      ]
    },
    "subagent": {
      "allowedAgents": [
        "explorer",
        "librarian",
        "oracle",
        "planner",
        "frontend-specialist",
        "document-writer",
        "general",
        "metis",
        "momus",
        "multimodal-looker",
        "browser-automation"
      ]
    }
  },  
  "allowedTools": [
    "read",
    "shell",
    "web_search",
    "web_fetch",
    "grep",
    "glob",
    "todo_list",
    "thinking",
    "subagent",
    "introspect",
    "report"
  ],
  "resources": [
    "skill://git-master",
    "skill://frontend-ui-ux",
    "skill://agent-browser",
    "skill://playwright",
    "skill://dev-browser"
  ]
}
