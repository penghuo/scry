{
  "default_agent": "sisyphus",
  "tmux": {
    "enabled": true,
    "layout": "main-vertical"
  },
  "claude_code": {
    "mcp": true,
    "commands": true,
    "skills": true,
    "agents": true,
    "hooks": true,
    "plugins": true,
    "plugins_override": {}
  },
  "disabled_hooks": [],
  "disabled_skills": [],
  "experimental": {
    "preemptive_compaction": true,
    "aggressive_truncation": false,
    "auto_resume": true,
    "dcp": true
  },
  "paths": {
    "todos": "~/.claude/todos",
    "transcripts": "~/.claude/transcripts"
  },
  "mcp": {
    "use_builtin": true,
    "include_claude_code": true
  }
}
