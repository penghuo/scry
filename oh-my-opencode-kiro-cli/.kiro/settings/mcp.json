{
  "servers": {
    "websearch": {
      "command": "bunx",
      "args": ["oh-my-opencode", "mcp", "websearch"],
      "env": {}
    },
    "context7": {
      "command": "bunx",
      "args": ["oh-my-opencode", "mcp", "context7"],
      "env": {}
    },
    "grep_app": {
      "command": "bunx",
      "args": ["oh-my-opencode", "mcp", "grep-app"],
      "env": {}
    },
    "playwright": {
      "command": "npx",
      "args": ["@anthropic-ai/mcp-playwright"],
      "env": {}
    }
  },
  "oauth": {
    "token_store": "~/.config/opencode/mcp-oauth.json"
  }
}
